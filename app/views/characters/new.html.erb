<div class="form">
	<%= form_for @character do |f| %>
	  <%= f.label :name %>
	  <%= f.text_field :name %>

	  <%= f.label :img_url %>
	  <%= f.text_field :img_url %>

	  <%= f.label :house_id %>
	  <%= f.select :house_id, options_for_select(House.all.map{ |i| [i.name, i.id] }) %>

	  <%= f.submit %>
	<% end %>
</div>